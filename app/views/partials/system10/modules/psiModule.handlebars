                            </label>
                        </div>

                        <div class="w-100"></div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-9" autocomplete="off" onchange="psiCheck(9)">
                            <label class="btn btn-outline-gama" id="label-check-psi-9" for="btn-check-psi-9">
                                <img class="img-fluid" src="/system10/assets/kappa/diode.png">
                                <img class="img-fluid" src="/system10/assets/alfa/suu.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-10" autocomplete="off" onchange="psiCheck(10)">
                            <label class="btn btn-outline-gama" id="label-check-psi-10" for="btn-check-psi-10">
                                <img class="img-fluid" src="/system10/assets/kappa/l.png">
                                <img class="img-fluid" src="/system10/assets/alfa/srr.png">
                            </label>
                        </div>

