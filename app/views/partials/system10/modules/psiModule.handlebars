                            <label class="btn btn-outline-gama" id="label-check-psi-12" for="btn-check-psi-12">
                                <img class="img-fluid" src="/system10/assets/kappa/lac.png">
                            </label>
                        </div>

                        <div class="w-100"></div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-13" autocomplete="off" onchange="psiCheck(13)" checked disabled>
                            <label class="btn btn-outline-gama" id="label-check-psi-13" for="btn-check-psi-13">
                                <img class="img-fluid" src="/system10/assets/alfa/rrr.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-14" autocomplete="off" onchange="psiCheck(14)">
                            <label class="btn btn-outline-gama" id="label-check-psi-14" for="btn-check-psi-14">
                                <img class="img-fluid" src="/system10/assets/kappa/nor.png">
                                <img class="img-fluid" src="/system10/assets/kappa/not.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-15" autocomplete="off" onchange="psiCheck(15)">
                            <label class="btn btn-outline-gama" id="label-check-psi-15" for="btn-check-psi-15">
                                <img class="img-fluid" src="/system10/assets/kappa/dc.png">
                                <img class="img-fluid" src="/system10/assets/kappa/mosfet.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
