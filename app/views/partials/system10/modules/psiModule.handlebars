<div class="modal fade" id="{{name}}Module" tabindex="-1">
    <div class="modal-dialog modal-lg modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header justify-content-center">
                <h4 class="modal-title text-center">{{name}}</h4>
            </div>

            {{#if isDone}}
                <div id="{{name}}BodyDone" class="modal-body">
                    <div class="row justify-content-center text-center" style="font-size: 25px">
                        <h4>Módulo Finalizado</h4>
                    </div>
                </div>
            {{else}}
                <div id="{{name}}BodyNot" class="modal-body">

                    <div class="position-absolute" style="right: 1%; top: 1%; z-index: 10">
                        <button class="btn btn-lg btn-outline-light" onclick="psiReset()">
                            <i class="fas fa-redo-alt"></i>
                        </button>
                    </div>

                    <div class="row justify-content-center text-center mx-0" style="font-size: 25px">

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-1" autocomplete="off" onchange="psiCheck(1)">
                            <label class="btn btn-outline-gama" id="label-check-psi-1" for="btn-check-psi-1">
                                <img class="img-fluid" src="/system10/assets/kappa/lmosfet.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-2" autocomplete="off" onchange="psiCheck(2)">
                            <label class="btn btn-outline-gama" id="label-check-psi-2" for="btn-check-psi-2">
                                <img class="img-fluid" src="/system10/assets/kappa/c.png">
                                <img class="img-fluid" src="/system10/assets/kappa/and.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-3" autocomplete="off" onchange="psiCheck(3)">
                            <label class="btn btn-outline-gama" id="label-check-psi-3" for="btn-check-psi-3">
                                <img class="img-fluid" src="/system10/assets/kappa/ac.png">
                                <img class="img-fluid" src="/system10/assets/kappa/diode.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-4" autocomplete="off" onchange="psiCheck(4)">
                            <label class="btn btn-outline-gama" id="label-check-psi-4" for="btn-check-psi-4">
                                <img class="img-fluid" src="/system10/assets/kappa/lnand.png">
                            </label>
                        </div>

                        <div class="w-100"></div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-5" autocomplete="off" onchange="psiCheck(5)">
                            <label class="btn btn-outline-gama" id="label-check-psi-5" for="btn-check-psi-5">
                                <img class="img-fluid" src="/system10/assets/alfa/rrr.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-6" autocomplete="off" onchange="psiCheck(6)">
                            <label class="btn btn-outline-gama" id="label-check-psi-6" for="btn-check-psi-6">
                                <img class="img-fluid" src="/system10/assets/alfa/sl.png">
                                <img class="img-fluid" src="/system10/assets/kappa/dc.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-7" autocomplete="off" onchange="psiCheck(7)">
                            <label class="btn btn-outline-gama" id="label-check-psi-7" for="btn-check-psi-7">
                                <img class="img-fluid" src="/system10/assets/kappa/and.png">
                                <img class="img-fluid" src="/system10/assets/kappa/l.png">
                            </label>
                        </div>

                        <div class="col-2 ps-0 pe-0">
                            <input type="checkbox" class="btn-check" id="btn-check-psi-8" autocomplete="off" onchange="psiCheck(8)">
