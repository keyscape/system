                            <label class="btn btn-outline-gama" id="label-check-psi-16" for="btn-check-psi-16">
                                <img class="img-fluid" src="/system10/assets/kappa/lc.png">
                            </label>
                        </div>

                    </div>
                </div>

                <div id="{{name}}BodyDone" class="modal-body d-none">
                    <div class="row justify-content-center text-center" style="font-size: 25px">
                        <h4>Módulo Finalizado</h4>
                    </div>
                </div>

            {{/if}}
        </div>
    </div>
</div> 


