
<div id="backDiv" class="background background-custom {{className}} {{bgc}} vh-100">
    <div class="container-fluid h-100">

        <div class="row text-center mx-0" id="boxAll">

            <div class="col-12 border border-custom {{className}} position-relative">

                <div class="position-absolute myRecomends" style="right: 1%; top: 1%">

                    <input type="checkbox" class="btn-check" id="btn-check-back" autocomplete="off" onclick="staticBack(this.checked)">
                    <label class="btn btn-outline-danger" for="btn-check-back"><i class="fas fa-ban"></i></label><br>

                </div>

                <div style="height: 10%;" class="d-flex justify-content-center align-items-center">
                    <a role="button" data-bs-toggle="modal" data-bs-target="#infoCPU">
                        <h3 class="title-main {{className}}">Unidade Central de Processamento</h3>
                    </a>
                </div>

                <div class="row justify-content-center align-items-center" style="height: 85%;">

                    {{#if phaseOne}}
                        {{> system10/mainModules modules.omega }}
                        {{> system10/mainModules modules.delta }}

                        <div class="w-100"></div>
                    {{/if}}

                    {{> system10/allModules modules.mi }}
                    {{> system10/allModules modules.zeta }}
                    {{> system10/allModules modules.beta }}

                    <div class="w-100"></div>

                    {{> system10/allModules modules.sigma }}
                    {{> system10/allModules modules.epsilon }}

                    <div class="w-100"></div>

                    {{> system10/allModules modules.alfa }}
                    {{> system10/allModules modules.psi }}
                    {{> system10/allModules modules.lambda }}

                    <div class="w-100"></div>

                    {{> system10/allModules modules.gama }}
                    {{> system10/allModules modules.kappa }} 


                </div>
                    
            </div>
        </div>
    </div>

    <div class="modal fade" id="infoCPU" tabindex="-1">
        <div class="modal-dialog modal-lg modal-dialog-centered">
            <div class="modal-content {{className}}">
                {{#if isDone}}
                    <div class="modal-header justify-content-center {{className}}">
                        <h4 class="modal-title text-center">UNIDADE CENTRAL DE PROCESSAMENTO</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row justify-content-center text-center">
                            <div class="col-sm-4 mb-3">
                                <div class="small text-white-50">NOME:</div>
                                <div>EULER</div>
                            </div>

                            <div class="col-sm-4 mb-3">
                                <div class="small text-white-50">TIPO:</div>
                                <div>IA</div>
                            </div>

                            <div class="col-sm-4 mb-3">
                                <div class="small text-white-50">CLASSE:</div>
                                <div>X+++</div>
                            </div>

                            <div class="col-sm-4 mb-3">
                                <div class="small text-white-50">PROPRIEDADE:</div>
                                <div>EXPOENTE</div>
                            </div>

                            <div class="col-sm-4 mb-3">
                                <div class="small text-white-50">PROG-N:</div>
                                <div>{{userName}}-{{groupNum}}</div>
                            </div>

                            <div class="col-sm-12 mb-3">
                                <div class="small text-white-50">STATUS:</div>
                                <div>OPERANTE</div>
                            </div>

                            <div class="col-sm-12 mb-3">
                                {{{messageProblem}}}
                            </div>

                        </div>

                    </div>
                {{else}}
